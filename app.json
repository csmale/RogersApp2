{
  "expo": {
    "name": "GateMaster",
    "slug": "GateMaster",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with other users.",
          "cameraPermission": "The app accesses your camera to let you share photos with other users."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],

    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.colinsmale.gatemaster"
    },
    "android": {
      "package": "com.colinsmale.gatemaster",
      "adaptiveIcon": {
        "foregroundImage": "./images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBqJGt51YpttKhVRATWqAR7Il4MjrzhwS8"
        }
      }
    },
    "extra": {
      "web": {
        "favicon": "./images/favicon.png"
      },
      "eas": {
        "projectId": "8055e536-b18b-4b04-8e3b-dc10db188d44"
      }
    }
  }
}